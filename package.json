{
  "name": "cinas",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "mocha",
    "test:watch": "mocha --watch",
    "build": "babel modules --out-dir dist/modules && babel src --out-dir dist/public/js",
    "postbuild": "vite build",
    "start": "node App.js",
    "dev": "concurrently -n \"VITE,EXPRESS\" -c \"blue,green\" \"vite\" \"tsx src/server.ts\""
  },
  "dependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@types/express": "^5.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-loader": "^9.1.3",
    "bcrypt": "^5.1.1",
    "express": "^4.21.2",
    "jsdom": "^24.0.0",
    "mongodb": "6.13",
    "mongoose": "^8.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.2.11",
    "vite-plugin-node": "^4.0.0"
  },
  "devDependencies": {
    "@babel/node": "^7.23.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "mocha": "^10.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
